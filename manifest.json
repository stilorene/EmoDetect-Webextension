{
  "manifest_version": 3,
  "name": "Productivity Tool (with little extra)",
  "version": "1.6",
  "description": "Simply make your time valuable and fun.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "popup/chat-pfeil-wachsen.png"
  },
  "permissions": [
    "storage"
  ],

  "content_security_policy": {
    "script-src": "self",
    "object-src": "self"
  }
}
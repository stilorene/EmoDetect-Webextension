{
  "manifest_version": 3,
  "name": "Productivity Timer",
  "version": "1.5",
  "description": "A simple productivity timer with note-taking feature.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "popup/website.png"
  },
  "permissions": [
    "storage"
  ]
}